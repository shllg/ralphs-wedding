generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
}

model WeddingEvent {
  id             String   @id @default(cuid())
  name           String
  eventDate      DateTime
  location       String
  invitationText String   @db.Text
  createdAt      DateTime @default(now())
  updatedAt      DateTime @updatedAt
}
